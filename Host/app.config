<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="AuthorBehavior">
                    <serviceMetadata httpGetEnabled="true"/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="AuthorBehavior" name="Service.AuthorService">
                <endpoint address="AuthorService" binding="netTcpBinding" bindingConfiguration="" contract="Service.ICommonService`1"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:8080"/>
                        <add baseAddress="net.tcp://localhost:8090"/>
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>
